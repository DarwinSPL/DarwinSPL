<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="eu.hyvar.feature.graphical.editor.editor.HyGraphicalFeatureModelEditor"
            contributorClass="eu.hyvar.feature.graphical.editor.editor.HyGraphicalFeatureModelEditorActionBarContributor"
            default="true"
            extensions="hyfeature, dwfeaturemodel"
            icon="icons/icon_standard.png"
            id="eu.hyvar.feature.graphical.editor.evolution.editor"
            name="DarwinSPL Feature Model Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.darwinspl.wizards"
            class="eu.hyvar.feature.graphical.editor.wizard.FeatureModelWizard"
            icon="icons/icon_standard.png"
            id="eu.hyvar.feature.graphical.editor.new_fm_wizard"
            name="DarwinSPL Feature Model">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="eu.hyvar.feature.graphical.editor.toolbar1">
            <command
                  commandId="eu.hyvar.feature.graphical.editor.command1"
                  icon="icons/icon_standard.png"
                  label="Create FM Constraints"
                  style="push"
                  tooltip="Create FM Constraints">
            </command>
            <command
                  commandId="eu.hyvar.feature.graphical.editor.command2"
                  icon="icons/icon_standard.png"
                  label="Create FM Context Information"
                  style="push"
                  tooltip="Create FM Context Information">
            </command>
            <command
                  commandId="eu.hyvar.feature.graphical.editor.command3"
                  icon="icons/icon_standard.png"
                  label="Create FM Validity Model"
                  style="push"
                  tooltip="Create FM Validity Model">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
</plugin>
