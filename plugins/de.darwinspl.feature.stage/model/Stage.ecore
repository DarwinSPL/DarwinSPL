<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="stage" nsURI="https://darwinspl.de/stage/1.0" nsPrefix="stage">
  <eClassifiers xsi:type="ecore:EClass" name="StageModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="featureModel" lowerBound="1"
        eType="ecore:EClass ../../eu.hyvar.feature/model/Feature.ecore#//HyFeatureModel"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roles" lowerBound="1" upperBound="-1"
        eType="#//Role" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stageOrder" lowerBound="1"
        upperBound="-1" eType="#//StageOrder" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stages" lowerBound="1"
        upperBound="-1" eType="#//Stage" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Stage" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyTemporalElement ../../eu.hyvar.evolution/model/Evolution.ecore#//HyNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="composition" upperBound="-1"
        eType="#//StageComposition" containment="true" eOpposite="#//StageComposition/compositionOf"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roleAssignment" lowerBound="1"
        upperBound="-1" eType="#//RoleAssignment" containment="true" eOpposite="#//RoleAssignment/stage"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Role" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyTemporalElement ../../eu.hyvar.evolution/model/Evolution.ecore#//HyNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="inclusions" upperBound="-1"
        eType="#//RoleInclusion" containment="true" eOpposite="#//RoleInclusion/inclusionsFor"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="includedBy" upperBound="-1"
        eType="#//RoleInclusion" eOpposite="#//RoleInclusion/includes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignments" upperBound="-1"
        eType="#//RoleAssignment" eOpposite="#//RoleAssignment/roles"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RoleInclusion" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyLinearTemporalElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="includes" upperBound="-1"
        eType="#//Role" eOpposite="#//Role/includedBy"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="inclusionsFor" lowerBound="1"
        eType="#//Role" eOpposite="#//Role/inclusions"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StageComposition" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyTemporalElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="features" upperBound="-1"
        eType="ecore:EClass ../../eu.hyvar.feature/model/Feature.ecore#//HyFeature"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="groups" upperBound="-1"
        eType="ecore:EClass ../../eu.hyvar.feature/model/Feature.ecore#//HyGroup"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="compositionOf" lowerBound="1"
        eType="#//Stage" eOpposite="#//Stage/composition"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RoleAssignment" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyLinearTemporalElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="stage" lowerBound="1" eType="#//Stage"
        eOpposite="#//Stage/roleAssignment"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roles" lowerBound="1" upperBound="-1"
        eType="#//Role" eOpposite="#//Role/assignments"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StageOrder" eSuperTypes="../../eu.hyvar.evolution/model/Evolution.ecore#//HyLinearTemporalElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="stages" lowerBound="1"
        upperBound="-1" eType="#//Stage"/>
  </eClassifiers>
</ecore:EPackage>
