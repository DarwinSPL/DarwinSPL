{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": " Feature Model, Mapping, Configuration",
  "type": "object",
  "properties": {
    "msg_type": {
      "description": "type of message",
      "type": "string"
    },
    "featureModel": {
      "description": "feature model",
      "type": "object",
      "properties": {
        "name": {
          "description": "name of the feature model",
          "type": "string"
        },
        "content": {
          "description": "content of the feature model",
          "type": "string"
        }
      }
    },
    "mapping": {
      "description": "mapping of delta modules to features",
      "type": "object",
      "properties": {
        "name": {
          "description": "name of the mapping",
          "type": "string"
        },
        "content": {
          "description": "content of the mapping",
          "type": "string"
        }
      }
    },
    "configuration": {
      "description": "configuration",
      "type": "object",
      "properties": {
        "name": {
          "description": "name of the mapping",
          "type": "string"
        },
        "content": {
          "description": "content of the mapping",
          "type": "string"
        }
      }
    }
  }
}
